<p id="notice"><%= notice %></p>

<h1>Listing Newsletter Emails</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @newsletter_emails.each do |newsletter_email| %>
      <tr>
        <td><%= newsletter_email.email %></td>
        <td><%= newsletter_email.product %></td>
        <td><%= link_to 'Show', newsletter_email %></td>
        <td><%= link_to 'Edit', edit_newsletter_email_path(newsletter_email) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Newsletter email', new_newsletter_email_path %>
